{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
		"isolatedModules": true,
		"noEmit": true,
		"allowImportingTsExtensions": true,
  },
	"include": ["src/**/*.tsx", "src/**/*.ts","src/**/*.mdx", "src/client/homePage.ts"]
}